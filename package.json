{
  "name": "dynastyweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev ",
    "dev:prod": "NODE_ENV=production next dev --turbopack",
    "dev:emulator": "concurrently \"cd ../dynastyfirebase/functions && npm run build:watch\" \"cd ../dynastyfirebase && firebase emulators:start --import=./emulator-data --export-on-exit\" \"next dev --turbopack\" --rules",
