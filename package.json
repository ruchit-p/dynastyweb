    "start": "next start",
    "lint": "next lint",
    "emulator:export": "cd ../dynastyfirebase && firebase emulators:export ./emulator-data",
    "emulator:start": "cd ../dynastyfirebase && firebase emulators:start --import=./emulator-data --export-on-exit"
  },
